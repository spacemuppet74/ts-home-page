{
  "compilerOptions": {
    "target": "ES6",
    "experimentalDecorators": true,
    "baseUrl": "src",
    "paths": {
      "components/*": [
        "./components/*"
      ],
      "models/*": [
        "./models/*"
      ],
      "pages/*": [
        "./pages/*"
      ],
      "store/*": [
        "./stores/*"
      ],
      "services/*": [
        "./services/*"
      ],
      "utils/*": [
        "./utils/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}